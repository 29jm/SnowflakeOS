    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Attach",
            "type": "gdb",
            "request": "attach",
            "target": "localhost:1234",
            "remote": true,
            "executable": "${workspaceFolder}/kernel/SnowflakeOS.kernel",
            "cwd": "${workspaceFolder}",
            "printCalls": true
        }
    ]
}